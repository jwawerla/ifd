# Robots
define autorob pioneer2dx
(
 blobfinder(range 1.0 )
 gripper(pose [0.2 0 -0.15 0])
 sicklaser( samples 32 range_max 5 laser_return 2 watts 30 )
 ctrl "ifd"
 joules 1000000
 joules_capacity 400000
 #fiducial_return 0
)

