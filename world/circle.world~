include "pioneer.inc"
include "map.inc"
include "sick.inc"
include "patch.inc"

speedup 10.000
paused 1

# time at which to pause (in GUI mode) or quit (in headless mode) the simulation
#quit_time 3600 # 1 hour of simulated time

resolution 0.02

#threads 0

# configure the GUI window
window
(
  size [ 683.000 713.000 ]

  center [ 2.100 1.547 ]
  rotate [ 0 0 ]
  scale 11.243

  show_data 1
  show_flags 1

  interval 50
)

# load an environment bitmap
floorplan
(
  name "circle"
  pose [0 0 0 0]
  size [65.000 65.000 0.600]
  bitmap "circle.png"
  #bitmap "cave.png"
)


model
(
  ctrl "patchManagerCtrl"
  name "patchManagerCtrl"
  pose [ 30.000 30.000 0 0 ]
  size [1.0 1.0 1.0]
)

###############################################################################
# patches
patch
(
  db_count 4
  db [ "puck_rate<float>0.1" "max_pucks<int>50" "radius<float>7.25" "init_empty<int>0"]
  pose [ 0.37 24.72 0 0 ]
  name "patchA"
)

